{
  "manifest_version": 3,
  "name": "Work log",
  "version": "1.0",
  "description": "Automatically sets input value on page refresh",
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "JIRA.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
